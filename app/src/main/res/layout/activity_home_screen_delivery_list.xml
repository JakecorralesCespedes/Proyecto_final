<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fondo"
    tools:context=".Home_Screen_Delivery_list">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:id="@+id/linearLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="50dp"
                    android:layout_marginEnd="50dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/delivery"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        app:srcCompat="@drawable/delivery" />

                    <ImageView
                        android:id="@+id/pickup"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        app:srcCompat="@drawable/pickup" />

                    <ImageView
                        android:id="@+id/dine_in"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        app:srcCompat="@drawable/dini_in" />

                </LinearLayout>

                <ImageView
                    android:id="@+id/grocery"
                    android:layout_width="183.69dp"
                    android:layout_height="110.97dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginEnd="16dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayout"
                    app:srcCompat="@drawable/grocery" />

                <ImageView
                    android:id="@+id/alcohol"
                    android:layout_width="92.07dp"
                    android:layout_height="95.17dp"

                    android:layout_marginTop="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/pet_supplies"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toEndOf="@+id/convenience"
                    app:layout_constraintTop_toBottomOf="@+id/american_food"
                    app:srcCompat="@drawable/alcohol" />

                <ImageView
                    android:id="@+id/pet_supplies"
                    android:layout_width="92.07dp"
                    android:layout_height="95.17dp"
                    android:layout_marginTop="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.436"
                    app:layout_constraintStart_toEndOf="@+id/convenience"
                    app:layout_constraintTop_toBottomOf="@+id/grocery"
                    app:srcCompat="@drawable/pet_supplies" />

                <ImageView
                    android:id="@+id/more"
                    android:layout_width="92.07dp"
                    android:layout_height="95.17dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="12dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/grocery"
                    app:srcCompat="@drawable/more" />

                <ImageView
                    android:id="@+id/convenience"
                    android:layout_width="92.07dp"
                    android:layout_height="95.17dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="8dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/american_food"
                    app:srcCompat="@drawable/convenience" />

                <ImageView
                    android:id="@+id/american_food"
                    android:layout_width="183.69dp"
                    android:layout_height="94.97dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="28dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayout"
                    app:srcCompat="@drawable/american_food" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <ImageView
                android:id="@+id/wendys"
                android:layout_width="wrap_content"
                android:layout_height="280dp"
                app:srcCompat="@drawable/wendy_s" />

            <ImageView
                android:id="@+id/tacontento"
                android:layout_width="wrap_content"
                android:layout_height="280dp"
                app:srcCompat="@drawable/tacontento" />

            <ImageView
                android:id="@+id/kfc"
                android:layout_width="wrap_content"
                android:layout_height="280dp"
                app:srcCompat="@drawable/kfc" />


            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >

                    <ImageView
                                android:id="@+id/promo_scroll_1"
                                android:layout_width="337.25dp"
                                android:layout_height="166.55dp"
                                app:srcCompat="@drawable/promo_scroll_1" />

                    <ImageView
                        android:id="@+id/promo_scroll_2"
                        android:layout_width="337.25dp"
                        android:layout_height="166.55dp"
                        android:layout_marginStart="15dp"
                        app:srcCompat="@drawable/promo_scroll_2" />
                </LinearLayout>
            </HorizontalScrollView>

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="match_parent"
                android:layout_height="270dp"
                app:srcCompat="@drawable/nacionsushi_cr" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="match_parent"
                android:layout_height="270dp"
                app:srcCompat="@drawable/subway" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="35dp"
                android:layout_marginBottom="20dp"
                android:layout_marginEnd="35dp"

                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView14"
                    android:layout_width="171dp"
                    android:layout_height="66dp"
                    android:layout_marginEnd="10dp"
                    app:srcCompat="@drawable/caribe" />

                <ImageView
                    android:id="@+id/imageView15"
                    android:layout_width="168dp"
                    android:layout_height="66dp"
                    app:srcCompat="@drawable/asiatica" />



            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="35dp"
                android:layout_marginEnd="35dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView16"
                    android:layout_width="171dp"
                    android:layout_height="66dp"
                    app:srcCompat="@drawable/pizza" />

                <ImageView
                    android:id="@+id/imageView17"
                    android:layout_width="168dp"
                    android:layout_height="66dp"
                    app:srcCompat="@drawable/india" />
            </LinearLayout>

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="wrap_content"
                android:layout_height="265.8dp"
                app:srcCompat="@drawable/spoon" />

            <ImageView
                android:id="@+id/moyo"
                android:layout_width="wrap_content"
                android:layout_height="265.8dp"
                app:srcCompat="@drawable/moyo" />

            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="377dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:srcCompat="@drawable/navegar_o_buscar" />

            <ImageView
                android:id="@+id/info_comerci"
                android:layout_width="377dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:srcCompat="@drawable/info_marketing" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView20"
                    android:layout_width="28dp"
                    android:layout_height="39dp"
                    android:layout_marginEnd="45dp"
                    app:srcCompat="@drawable/home" />

                <ImageView
                    android:id="@+id/imageView21"
                    android:layout_width="118dp"
                    android:layout_height="39dp"
                    android:layout_marginEnd="45dp"
                    app:srcCompat="@drawable/grocery_and_browse" />

                <ImageView
                    android:id="@+id/imageView22"
                    android:layout_width="36dp"
                    android:layout_height="39dp"
                    android:layout_marginEnd="45dp"
                    app:srcCompat="@drawable/carrito" />

                <ImageView
                    android:id="@+id/imageView23"
                    android:layout_width="40dp"
                    android:layout_height="38dp"
                    app:srcCompat="@drawable/cuenta" />
            </LinearLayout>

        </LinearLayout>
            </ScrollView>



</androidx.constraintlayout.widget.ConstraintLayout>