        android:layout_height="30dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/tacos1"
        app:layout_constraintEnd_toEndOf="@+id/tacos1"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/tacos1"
        app:layout_constraintTop_toTopOf="@+id/tacos1"
        app:layout_constraintVertical_bias="0.879"
