        android:id="@+id/imageButton3"
        android:layout_width="36dp"
        android:layout_height="30dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/nachos2"
        app:layout_constraintEnd_toEndOf="@+id/nachos2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/nachos2"
        app:layout_constraintTop_toTopOf="@+id/nachos2"
